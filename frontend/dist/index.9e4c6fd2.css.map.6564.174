tion-item {\r\n  color: var(--primary-color) !important; /* Orange selected option text */\r\n}\r\n\r\n\r\n/* styles.css */\r\n.custom-row {\r\n  height: 22px; /* Adjust this value to your preference */\r\n}\r\n\r\n.custom-row td {\r\n  padding: 8px 16px; /* Adjust top/bottom and left/right padding as needed */\r\n}\r\n\r\n\r\n/* For Webkit browsers (Chrome, Safari, and Edge) */\r\n::-webkit-scrollbar {\r\n  width: 4px; /* Set the width for vertical scrollbars */\r\n  height: 8px; /* Set the height for horizontal scrollbars */\r\n}\r\n\r\n::-webkit-scrollbar-track {\r\n  background: #f1f1f1; /* Optional: background for the scrollbar track */\r\n}\r\n\r\n::-webkit-scrollbar-thumb {\r\n  background: var(--primary-color); /* Set the color of the scrollbar */\r\n  border-radius: 10px; /* Optional: round edges of the scrollbar */\r\n}\r\n\r\n::-webkit-scrollbar-thumb:hover {\r\n  background: var(--primary-color); /* Optional: hover color for the scrollbar */\r\n}\r\n\r\n\r\n.react-resizable {\r\n  position: relative;\r\n  background-clip: padding-box;\r\n}\r\n\r\n.react-resizable-handle {\r\n  position: absolute;\r\n  inset-inline-end: -5px;\r\n  bottom: 0;\r\n  z-index: 1;\r\n  width: 10px;\r\n  height: 100%;\r\n  cursor: col-resize;\r\n}\r\n\r\n\r\n/* button:disabled  */\r\nbutton:disabled {\r\n  background: #cccccc; /* Disabled state background */\r\n  cursor: not-allowed;  /* Disabled state cursor */\r\n  box-shadow: none;     /* Remove shadow when disabled */\r\n}\r\n\r\n.icons:disabled {\r\n  background: transparent; /* Disabled state background */\r\n  cursor: not-allowed;  /* Disabled state cursor */\r\n  box-shadow: none;     /* Remove shadow when disabled */\r\n}\r\n\r\n.icons svg {\r\n  stroke: #919191; /* Default color */\r\n}\r\n\r\n.icons.disabled-svg {\r\n  stroke: #d3d3d3; /* Lighter, disabled color */\r\n}\r\n\r\n\r\n\r\n/* loader inside a button */\r\n.loader {\r\n  border: 4px solid transparent; /* Transparent border for the full circle */\r\n  border-